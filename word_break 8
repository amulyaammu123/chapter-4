def word_break(s, wordDict):
    n = len(s)
    dp = [False] * (n + 1)
    dp[0] = True
    
    for i in range(1, n + 1):
        for word in wordDict:
            if dp[i - len(word)] and s[i - len(word):i] == word:
                dp[i] = True
                break
    
    return dp[n]

# Example usage
s = "leetcode"
wordDict = ["leet", "code"]
print("Input: s =", s)
print("wordDict =", wordDict)
print("Output:", word_break(s, wordDict))
